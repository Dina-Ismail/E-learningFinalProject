@model IEnumerable<E_learningFinalProject.Models.SemesterSchedule>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SemesterID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfessorID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MethodOfDelivery)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SemesterID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourseID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfessorID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MethodOfDelivery)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SemesterScheduleID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SemesterScheduleID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SemesterScheduleID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
